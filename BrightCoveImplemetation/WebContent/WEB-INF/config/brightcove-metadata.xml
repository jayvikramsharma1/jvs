<?xml version="1.0" encoding="UTF-8"?>
		 <!DOCTYPE doc SYSTEM "/SysConfig/Common/Rules/ni.dtd">
		 <?EM-dtdExt /SysConfig/Common/Rules/ni.dtx?>
		 <?EM-templateName /SysConfig/Common/Templates/General/VideoStory.video.xml?>
		 <?xml-stylesheet type="text/css" href="/SysConfig/Common/Rules/main.css"?>
		 <doc xml:lang="en-uk">
		     <story>
		     <!-- for times only grouhead with id=U+uuid is requred -->
		         <grouphead id="U%s">
		         <!-- for sunday times both headline and standfirst with id = U + uuid is required -->
		             <headline id="U%s">
		                 <p class="p">%s</p>
		             </headline>
		             <standfirst id="U%s">
		                 <p class="p">%s</p>
		             </standfirst>
		         </grouphead>
		         <!-- video-group with id = U + uuid is common for both sunday times and the times -->
		         <video-group id="U%s">
		            <video-thumbnail uuid="%s">
		 				<fw-photo-main class="image_tt_1600x900" description="main" fileref="%s" ratio="16:9" width="1600" height="900"/> 
		 				<fw-photo class="image_tt_1500x1000" description="digital" height="1000" width="1500" display-size="750" ratio="3:2"/> 	
		 				<fw-photo width="1000" height="1000" class="image_tt_1000x1000" description="digital" display-size="450" ratio="1:1"/>		
		 				<fw-photo width="1000" height="1500" class="image_tt_1000x1500" description="digital" display-size="300" ratio="2:3"/> 
		 				<fw-photo width="1200" height="1500" class="image_tt_1200x1500" description="digital" display-size="360" ratio="4:5"/> 
		 				<fw-photo width="1250" height="1000" class="image_tt_1250x1000" description="digital" display-size="300" ratio="1.25:1"/> 
		 			</video-thumbnail>
		             <video-caption>
		                 <p>%s</p>
		             </video-caption>
		             <video-credit>
		                 <p>%s</p>
		             </video-credit>
		         </video-group>
		     </story>
		 </doc>